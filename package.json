{
  "name": "node.js-typescript-starter",
  "version": "1.0.0",
  "description": "node.js-typescript-starter",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npx tsc",
    "clean": "rimraf dist coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --runInBand --colors --testPathPattern=test",
    "prepare": "husky install",
    "start": "npm i && npm run build && node dist/index.js"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ariesmcrae/node.js-typescript-starter.git"
  },
  "keywords": [
    "typescript",
    "node.js"
  ],
  "author": "XXX YYY <github@xxxyyy.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ariesmcrae/node.js-typescript-starter/issues"
  },
  "homepage": "https://github.com/ariesmcrae/node.js-typescript-starter#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3"
  }
}
